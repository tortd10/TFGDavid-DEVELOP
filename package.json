        "eslint-plugin-import": "^2.21.2",
        "eslint-plugin-sitegenesis": "~1.0.0",
        "globalize": "^1.4.2",
        "istanbul": "^0.4.5",
        "lodash": "^4.17.15",
        "minimist": "^1.2.0",
        "moment-timezone": "^0.5.26",
